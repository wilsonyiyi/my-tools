#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wilson/playgroud/my-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wilson/playgroud/my-tools/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@umijs+bundler-webpack@4.1.0_typescript@5.3.3/node_modules/@umijs/bundler-webpack/bin/bundler-webpack.js" "$@"
else
  exec node  "$basedir/../../../../../../@umijs+bundler-webpack@4.1.0_typescript@5.3.3/node_modules/@umijs/bundler-webpack/bin/bundler-webpack.js" "$@"
fi
