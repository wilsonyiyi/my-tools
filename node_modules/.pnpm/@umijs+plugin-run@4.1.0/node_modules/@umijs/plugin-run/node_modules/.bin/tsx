#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wilson/playgroud/my-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wilson/playgroud/my-tools/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tsx@3.14.0/node_modules/tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../tsx@3.14.0/node_modules/tsx/dist/cli.mjs" "$@"
fi
